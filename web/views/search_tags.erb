<div class="pre">
    <h1 class="section"><%= @title %></h1>
    <p><b><%= t.pages.search.results.you_were_searching_for %>:</b> <%= escape_html(@query) %></p>
</div>
<div id="tabs">
    <ul>
        <li><a href="#tags"><%= t.osm.tags %></a></li>
    </ul>
    <div id="tags">
        <h2><%= t.osm.tags %></h2>
        <table id="grid-tags">
        </table>
    </div>
</div>
<% javascript do
    JS.raw(%Q{
function page_init() {
    init_tabs('search', [#{ @query.to_json }]);
}
    })
end
%>
